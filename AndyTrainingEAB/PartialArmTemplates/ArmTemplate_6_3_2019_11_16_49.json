{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "AndyTrainingEAB"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/FoldersDoNotMatterInGit')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "this pipeline was created to illustrate that organizing pipelines in folders in ADF is not reflected in github source control",
				"activities": [
					{
						"name": "Wait 10",
						"type": "Wait",
						"typeProperties": {
							"waitTimeInSeconds": 10
						}
					}
				]
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/Wait Pipeline Changed Name')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Wait 5",
						"type": "Wait",
						"typeProperties": {
							"waitTimeInSeconds": 5
						}
					},
					{
						"name": "Wait 4",
						"type": "Wait",
						"dependsOn": [
							{
								"activity": "Wait 5",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"typeProperties": {
							"waitTimeInSeconds": 4
						}
					},
					{
						"name": "Wait 3",
						"type": "Wait",
						"dependsOn": [
							{
								"activity": "Wait 4",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"typeProperties": {
							"waitTimeInSeconds": 3
						}
					},
					{
						"name": "Wait 2",
						"type": "Wait",
						"dependsOn": [
							{
								"activity": "Wait 3",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"typeProperties": {
							"waitTimeInSeconds": 2
						}
					}
				]
			}
		}
	]
}